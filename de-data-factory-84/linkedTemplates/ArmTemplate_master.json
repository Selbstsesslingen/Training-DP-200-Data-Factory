{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "de-data-factory-84"
		},
		"AzureSynapseAnalyticsLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalyticsLinkedService'"
		},
		"awdlsstudde_LinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'awdlsstudde_LinkedService'"
		},
		"AzureDatabricksLinkedService_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksLinkedService'"
		},
		"HTTPSourceMovies_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/djpmsft/adf-ready-demo/master/moviesDB.csv"
		},
		"awdlsstudde_LinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://awdlsstudde.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSynapseAnalyticsLinkedService_connectionString": {
						"value": "[parameters('AzureSynapseAnalyticsLinkedService_connectionString')]"
					},
					"awdlsstudde_LinkedService_accountKey": {
						"value": "[parameters('awdlsstudde_LinkedService_accountKey')]"
					},
					"AzureDatabricksLinkedService_accessToken": {
						"value": "[parameters('AzureDatabricksLinkedService_accessToken')]"
					},
					"HTTPSourceMovies_properties_typeProperties_url": {
						"value": "[parameters('HTTPSourceMovies_properties_typeProperties_url')]"
					},
					"awdlsstudde_LinkedService_properties_typeProperties_url": {
						"value": "[parameters('awdlsstudde_LinkedService_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}